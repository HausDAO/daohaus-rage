{
  "root": "libs/local-abis",
  "sourceRoot": "libs/local-abis/src",
  "projectType": "library",
  "targets": {
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["libs/local-abis/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/libs/local-abis"],
      "options": {
        "jestConfig": "libs/local-abis/jest.config.js",
        "passWithNoTests": true
      }
    },
    "build": {
      "executor": "@nrwl/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libs/local-abis",
        "tsConfig": "libs/haus-sdk/tsconfig.lib.json",
        "packageJson": "libs/local-abis/package.json",
        "main": "libs/local-abis/src/index.ts",
        "assets": ["libs/local-abis/*.md"]
      }
    }
  },
  "tags": []
}
